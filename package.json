{
  "name": "mvom",
  "private": true,
  "version": "0.8.0",
  "description": "Multivalue Object Mapper",
  "main": "js/index.js",
  "engines": {
    "node": ">=8.16.0"
  },
  "scripts": {
    "build": "npm run build:node && npm run build:prepare-unibasic && npm run build:copy-files",
    "build:copy-files": "babel-node --extensions .ts,.js ./scripts/copy-files",
    "build:node": "babel ./src --env-name build --out-dir ./dist --extensions .ts,.js",
    "build:prepare-unibasic": "babel-node --extensions .ts,.js ./scripts/prepare-unibasic",
    "clean": "npm run clean:build",
    "clean:build": "rimraf ./dist",
    "docgen": "rimraf ./docs && documentation build src/** -f html -o docs --parse-extension js",
    "docgen:public": "rimraf ./docs && documentation build src/** --access public --access undefined -f html -o docs --parse-extension js",
    "docgen:private": "rimraf ./docs && documentation build src/** --access protected --access public --access private --access undefined -f html -o docs --parse-extension js",
    "lint": "eslint . && echo \"eslint: no lint errors\"",
    "lint:fix": "eslint . --fix && echo \"eslint: no lint errors\"",
    "prebuild": "npm run clean:build",
    "test": "npm run typecheck && npm run lint && npm run test:coverage:summary",
    "test:unit": "jest",
    "test:coverage": "jest --collectCoverage --coverageReporters text",
    "test:coverage:html": "jest --collectCoverage --coverageReporters html",
    "test:coverage:summary": "jest --collectCoverage --coverageReporters text-summary",
    "typecheck": "tsc --noEmit && echo \"tsc: no typecheck errors\""
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/storis/mvom.git"
  },
  "keywords": [
    "node",
    "multivalue",
    "database"
  ],
  "author": "STORIS",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "fs-extra": "^7.0.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "semver": "^6.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.6.0",
    "@babel/core": "7.6.0",
    "@babel/node": "7.6.1",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/preset-env": "7.6.0",
    "@babel/preset-typescript": "7.6.0",
    "@babel/register": "7.6.0",
    "@types/jest": "25.1.4",
    "@types/lodash": "4.14.138",
    "@types/node": "8.10.54",
    "@typescript-eslint/eslint-plugin": "2.3.0",
    "@typescript-eslint/parser": "2.3.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "25.1.0",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-rewire-ts": "1.3.2",
    "documentation": "12.1.2",
    "eslint": "6.4.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-import-resolver-babel-module": "5.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "jest": "25.1.0",
    "nunjucks": "3.2.1",
    "prettier": "1.18.2",
    "rimraf": "3.0.0",
    "sinon": "7.4.2",
    "typescript": "3.6.3"
  },
  "types": "./src/index.d.ts"
}
