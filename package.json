{
  "name": "mvom",
  "private": true,
  "version": "0.0.0",
  "description": "Multivalue Object Mapper",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build:node",
    "build:node": "cross-env NODE_ENV=production babel ./src --ignore *.spec.js --out-dir ./dist",
    "clean": "npm run clean:build",
    "clean:build": "rimraf ./dist",
    "docgen": "rimraf ./docs && documentation build src/** -f html -o docs --parse-extension js",
    "lint": "eslint . --cache && echo \"eslint: no lint errors\"",
    "lint:fix": "eslint . --fix && echo \"eslint: no lint errors\"",
    "prepare": "npm run build",
    "prebuild": "npm run clean:build",
    "test": "npm run lint && cross-env NODE_ENV=test BABEL_ENV=coverage nyc --reporter=text-summary --check-coverage mocha",
    "test:unit": "cross-env NODE_ENV=test mocha",
    "test:coverage": "cross-env NODE_ENV=test BABEL_ENV=coverage nyc mocha",
    "test:coverage:html": "npm run test:coverage && nyc report --reporter=html"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/storis/mvom.git"
  },
  "keywords": [
    "node",
    "multivalue",
    "database"
  ],
  "author": "STORIS",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^0.17.1",
    "semver": "^5.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-plugin-inline-import": "^2.0.6",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.1.3",
    "documentation": "^5.3.5",
    "eslint": "^4.14.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.4.0",
    "lodash": "^4.17.4",
    "mocha": "^4.1.0",
    "nyc": "^11.4.1",
    "prettier": "^1.9.2",
    "rimraf": "^2.6.2",
    "sinon": "^4.1.3"
  },
  "peerDependencies": {
    "lodash": ">=4.17.0"
  }
}
